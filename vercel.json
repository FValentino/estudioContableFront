{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "build" }
    }
  ],
  "routes": [
    { "src": "/", "dest": "/index.html" },
    { "src": "/servicios", "dest": "/index.html" },
    { "src": "/contacto", "dest": "/index.html" },
    { "src": "/suscribirse", "dest": "/index.html" },
    { "src": "/politica-de-privacidad", "dest": "/index.html" },
    { "src": "/terminos-y-condiciones", "dest": "/index.html" }
  ]
}

